    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-semistandard": "^17.0.0",
    "husky": "^8.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.9.0",
    "sinon": "^15.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "tonicExampleFilename": "./examples/signedURL.js"
